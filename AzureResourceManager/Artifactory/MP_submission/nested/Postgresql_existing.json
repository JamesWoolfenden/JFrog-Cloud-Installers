{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "outputs": {
    "databases": {
      "type": "object",
      "value": "[parameters('databases')]"
    },
    "db_edition": {
      "type": "string",
      "value": "[parameters('db_edition')]"
    },
    "db_location": {
      "type": "string",
      "value": "[parameters('db_location')]"
    },
    "db_name": {
      "type": "string",
      "value": "[parameters('db_name')]"
    },
    "db_server": {
      "type": "string",
      "value": "[parameters('db_server')]"
    },
    "db_user": {
      "type": "string",
      "value": "[parameters('db_user')]"
    },
    "jdbcConnString": {
      "type": "string",
      "value": "[parameters('manual_db_url')]"
    }
  },
  "parameters": {
    "databases": {
      "type": "object"
    },
    "db_edition": {
      "allowedValues": [
        "Basic",
        "Standard",
        "Premium"
      ],
      "defaultValue": "Basic",
      "type": "string"
    },
    "db_location": {
      "type": "string"
    },
    "db_name": {
      "defaultValue": "artdb",
      "minLength": 1,
      "type": "string"
    },
    "db_password": {
      "type": "securestring"
    },
    "db_server": {
      "defaultValue": "xraypostgressrv",
      "minLength": 1,
      "type": "string"
    },
    "db_user": {
      "defaultValue": "artifactory",
      "minLength": 1,
      "type": "string"
    },
    "manual_db_url": {
      "type": "string"
    }
  },
  "resources": [],
  "variables": {}
}
